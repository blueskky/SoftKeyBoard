        <Key
            android:keyLabel="⑦"
            android:keyOutputText="⑦" />
        <Key
            android:keyLabel="⑧"
            android:keyOutputText="⑧" />
        <Key
            android:keyLabel="⑨"
            android:keyOutputText="⑨" />
        <Key
            android:keyEdgeFlags="right"
            android:keyLabel="⑩"
            android:keyOutputText="⑩" />
