        <Key
            android:keyLabel="》"
            android:keyOutputText="》" />


        <Key
            android:isRepeatable="true"
            android:codes="57419"
            android:isModifier="true"
            android:keyIcon="@drawable/last" />


        <Key
            android:isRepeatable="true"
            android:codes="57421"
            android:isModifier="true"
            android:keyIcon="@drawable/next" />


        <Key
            android:codes="-5"
            android:isRepeatable="true"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/keyboard_delete" />
    </Row>
</Keyboard>
